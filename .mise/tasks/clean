#!/usr/bin/env bash
#MISE description="Clean build artifacts"

set -e

# Colors for output
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
NC='\033[0m' # No Color

cd "$(dirname "$0")/../.." # Navigate to project root

echo -e "${YELLOW}Cleaning build artifacts...${NC}"

rm -rf build

echo -e "${GREEN}âœ“ Build directory cleaned${NC}"
